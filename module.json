{
  "id": "alexs-spotlight-manager",
  "title": "Alex's Spotlight Manager",
  "description": "This module provides awesome features for Foundry VTT.",
  "authors": [
    {
      "name": "Alex M.",
      "url": "https://github.com/Alex-M-B95"
    }
  ],
  "compatibility": {
    "minimum": "11",
    "verified": "12"
  },
  "esmodules": [
    "sources/init.js"
  ],
  "styles": [
    "styles/spotlight-manager.css"
  ],
